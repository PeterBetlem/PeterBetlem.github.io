@article{buckleyTerrestrialLaserScanning2008,
  title = {Terrestrial Laser Scanning in Geology: Data Acquisition, Processing and Accuracy Considerations},
  shorttitle = {Terrestrial Laser Scanning in Geology},
  author = {Buckley, Simon J. and Howell, John A. and Enge, H.D. and Kurz, Tobias H.},
