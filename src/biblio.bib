@article{ulviEffectDistributionNumbers2021,
  title = {The Effect of the Distribution and Numbers of Ground Control Points on the Precision of Producing Orthophoto Maps with an Unmanned Aerial Vehicle},
  author = {Ulvi, Ali},
  date = {2021-11-02},
  journaltitle = {Journal of Asian Architecture and Building Engineering},
