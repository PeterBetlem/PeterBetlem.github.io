  keywords = {3D plot,Fine-grained siliciclastics,Sedimentological evolution,Trace elements},
  file = {C\:\\Users\\Peter\\Zotero\\storage\\6K4M3CEL\\Dypvik and Harris - 2001 - Geochemical facies analysis of fine-grained silici.pdf;C\:\\Users\\Peter\\Zotero\\storage\\6F2NIA8X\\S0009254101002789.html}
}

