@article{smyrak-sikoraImpactGrowthFaults2021,
  title = {Impact of Growth Faults on Mixed Siliciclastic‐carbonate‐evaporite Deposits during Rift Climax and Reorganisation—{{Billefjorden Trough}}, {{Svalbard}}, {{Norway}}},
  author = {Smyrak‐Sikora, Aleksandra and Nicolaisen, Jakob B. and Braathen, Alvar and Johannessen, Erik P. and Olaussen, Snorre and Stemmerik, Lars},
