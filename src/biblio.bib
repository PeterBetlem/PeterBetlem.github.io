@article{reigstadSulfurOxidizingChemolithotrophicProteobacteria2011,
  title = {Sulfur-{{Oxidizing Chemolithotrophic Proteobacteria Dominate}} the {{Microbiota}} in {{High Arctic Thermal Springs}} on {{Svalbard}}},
  author = {Reigstad, Laila Johanne and Jorgensen, Steffen Leth and Lauritzen, Stein-Erik and Schleper, Christa and Urich, Tim},
  date = {2011-09-01},
  journaltitle = {Astrobiology},
  volume = {11},
