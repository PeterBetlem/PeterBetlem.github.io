@article{nielsenFluidFlowMethane2014,
  title = {Fluid Flow and Methane Occurrences in the {{Disko Bugt}} Area Offshore {{West Greenland}}: Indications for Gas Hydrates?},
  shorttitle = {Fluid Flow and Methane Occurrences in the {{Disko Bugt}} Area Offshore {{West Greenland}}},
  author = {Nielsen, Tove and Laier, Troels and Kuijpers, Antoon and Rasmussen, Tine L. and Mikkelsen, Naja E. and Nørgård-Pedersen, Niels},
  date = {2014-12-01},
  journaltitle = {Geo-Marine Letters},
  shortjournal = {Geo-Mar Lett},
  volume = {34},
  number = {6},
  pages = {511--523},
  issn = {1432-1157},
  doi = {10.1007/s00367-014-0382-2},
  url = {https://doi.org/10.1007/s00367-014-0382-2},
  urldate = {2020-06-10},
  abstract = {The present study is the first to directly address the issue of gas hydrates offshore West Greenland, where numerous occurrences of shallow hydrocarbons have been documented in the vicinity of Disko Bugt (Bay). Furthermore, decomposing gas hydrate has been implied to explain seabed features in this climate-sensitive area. The study is based on archive data and new (2011, 2012) shallow seismic and sediment core data. Archive seismic records crossing an elongated depression (20×35 km large, 575 m deep) on the inner shelf west of Disko Bugt (Bay) show a bottom simulating reflector (BSR) within faulted Mesozoic strata, consistent with the occurrence of gas hydrates. Moreover, the more recently acquired shallow seismic data reveal gas/fluid-related features in the overlying sediments, and geochemical data point to methane migration from a deeper-lying petroleum system. By contrast, hydrocarbon signatures within faulted Mesozoic strata below the strait known as the Vaigat can be inferred on archive seismics, but no BSR was visible. New seismic data provide evidence of various gas/fluid-releasing features in the overlying sediments. Flares were detected by the echo-sounder in July 2012, and cores contained ikaite and showed gas-releasing cracks and bubbles, all pointing to ongoing methane seepage in the strait. Observed seabed mounds also sustain gas seepages. For areas where crystalline bedrock is covered only by Pleistocene–Holocene deposits, methane was found only in the Egedesminde Dyb (Trough). There was a strong increase in methane concentration with depth, but no free gas. This is likely due to the formation of gas hydrate and the limited thickness of the sediment infill. Seabed depressions off Ilulissat Isfjord (Icefjord) previously inferred to express ongoing gas release from decomposing gas hydrate show no evidence of gas seepage, and are more likely a result of neo-tectonism.},
  langid = {english},
  file = {C\:\\Users\\Peter\\Zotero\\storage\\DFDGUY9S\\Nielsen et al. - 2014 - Fluid flow and methane occurrences in the Disko Bu.pdf}
}

@article{nilsenFjordShelfExchanges2008,
  title = {Fjord–Shelf Exchanges Controlled by Ice and Brine Production: {{The}} Interannual Variation of {{Atlantic Water}} in {{Isfjorden}}, {{Svalbard}}},
  shorttitle = {Fjord–Shelf Exchanges Controlled by Ice and Brine Production},
  author = {Nilsen, F. and Cottier, F. and Skogseth, R. and Mattsson, S.},
  date = {2008-08-15},
  journaltitle = {Continental Shelf Research},
  shortjournal = {Continental Shelf Research},
  volume = {28},
  number = {14},
  pages = {1838--1853},
  issn = {0278-4343},
  doi = {10.1016/j.csr.2008.04.015},
  url = {http://www.sciencedirect.com/science/article/pii/S0278434308001878},
  urldate = {2020-05-14},
  abstract = {Exchanges between oceanic and coastal waters are fundamental in setting the hydrography of arctic shelves and fjords. In West Spitsbergen, Atlantic Water from the West Spitsbergen Current exchanges with the seasonally ice-covered waters of the coast and fjords causing a major annual shift in hydrographic conditions. The extent to which Atlantic Water dominates the fjord systems shows significant interannual variability. Hydrographic sections taken between 1999 and 2005 from Isfjorden and the adjacent shelf have been analyzed to identify the causes of the variability in Atlantic Water occupation of the fjord system. By treating the fjord system as a coastal polynya and running a polynya model to quantify the salt release each winter, we conclude that the critical parameter controlling fjord–shelf exchange is the density difference between the fjord water masses and the Atlantic Water. We provide a full dynamical mechanism for the interaction between water masses at the fjord entrance to rationalize the interannual variability.},
  langid = {english},
  keywords = {Atlantic Water,Brine-enriched fjord water,Fjord–shelf exchange,Hydrography,Polynya,Sea ice},
  file = {C\:\\Users\\Peter\\Zotero\\storage\\CB97XG2H\\Nilsen et al. - 2008 - Fjord–shelf exchanges controlled by ice and brine .pdf;C\:\\Users\\Peter\\Zotero\\storage\\DFXWLBLX\\S0278434308001878.html}
}

