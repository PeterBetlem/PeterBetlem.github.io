@article{skogsethVariabilityDecadalTrends2019,
  title = {Variability and {{Decadal Trends}} in the {{Isfjorden}} ({{Svalbard}}) {{Ocean Climate}} and {{Circulation}} - an Indicator for {{Climate Change}} in the {{Arctic Ocean}}},
  author = {Skogseth, R. and Olivier, L. L. A. and Nilsen, F. and Jonassen, M. O.},
  date = {2019-12-01},
  journaltitle = {AGU Fall Meeting Abstracts},
  shortjournal = {AGU Fall Meeting Abstracts},
  volume = {43},
  url = {http://adsabs.harvard.edu/abs/2019AGUFM.C43D1519S},
  urldate = {2020-05-04},
  abstract = {Isfjorden, a broad Arctic fjord in western Spitsbergen, has shown  significant changes in hydrography and inflow of Atlantic Water (AW) the last decades that only recently have been observed in the Arctic Ocean north of Svalbard. Variability and trends in this fjord's climate and circulation are therefore analysed from observational and reanalysis data during the time period 1987 to 2017. Over this period, Isfjorden has experienced a shift in summer ocean structure, from having AW generally in the bottom layer before 2006, to having AW (with increasing thickness) higher up in the water column from 2006. This shift, and a concomitant shift to less fast ice in Isfjorden are linked to positive trends in the mean sea surface temperature (SST) and volume weighted mean temperature (VT) in winter (SSTw/VTw: 0.6 ± 0.1 / 0.9 ± 0.3 °C 10yr-1) and summer (SSTS/VTS: 0.6 ± 0.1°C 10yr-1). Hence, the local mean air temperature also shows a positive trend in winter (1.9 ± 0.4 °C 10yr-1) and summer (0.7 ± 0.1 °C 10yr-1) over the same time period. Moreover, a positive trend in volume weighted mean salinity in winter (0.21 ± 0.06 10yr-1) and summer (0.06 ± 0.05 10yr-1) suggests increased advection of AW as a main reason for Isfjorden's climate change during the studied time period. Further, local mean air temperature correlates significantly with sea ice cover, SST, and VT, revealing the impact of the fjord's climate on the local terrestrial climate. Isfjorden has changed from being an Arctic type fjord dominated by deep (Winter Deep) and intermediate (Winter Intermediate) thermal and haline convection with sea ice formation, to a more Atlantic type fjord dominated by deep thermal convection of Atlantic type water (Winter Open). The lack of sea ice and more AW intrusion on the shelf outside, result in denser shelf water in winter, leading warmer AW from West Spitsbergen Current on top. This study of trends and changes in processes governing the inflow to Isfjorden, demonstrates that Isfjorden and its ocean climate can be used as a thermometer for climate change in the Arctic Ocean.},
  keywords = {0720 Glaciers,0726 Ice sheets,0732 Icebergs,0750 Sea ice,CRYOSPHERE}
}

