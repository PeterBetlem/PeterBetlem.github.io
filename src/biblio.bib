@article{rabbelFieldAnaloguesRealistic2018a,
  title = {From Field Analogues to Realistic Seismic Modelling: A Case Study of an Oil-Producing Andesitic Sill Complex in the {{Neuquén Basin}}, {{Argentina}}},
  shorttitle = {From Field Analogues to Realistic Seismic Modelling},
  author = {Rabbel, Ole and Galland, Olivier and Mair, Karen and Lecomte, Isabelle and Senger, Kim and Spacapan, Juan Bautista and Manceda, Rene},
