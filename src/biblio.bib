@article{ewertowskiOperationalFrameworkRapid2019,
  title = {Operational {{Framework}} for {{Rapid}}, {{Very-high Resolution Mapping}} of {{Glacial Geomorphology Using Low-cost Unmanned Aerial Vehicles}} and {{Structure-from-Motion Approach}}},
  author = {Ewertowski, Marek W. and Tomczyk, Aleksandra M. and Evans, David J. A. and Roberts, David H. and Ewertowski, Wojciech},
  date = {2019-01},
  journaltitle = {Remote Sensing},
  volume = {11},
  number = {1},
  pages = {65},
  publisher = {{Multidisciplinary Digital Publishing Institute}},
  doi = {10.3390/rs11010065},
  url = {https://www.mdpi.com/2072-4292/11/1/65},
  urldate = {2020-09-14},
  abstract = {This study presents the operational framework for rapid, very-high resolution mapping of glacial geomorphology, with the use of budget Unmanned Aerial Vehicles and a structure-from-motion approach. The proposed workflow comprises seven stages: (1) Preparation and selection of the appropriate platform; (2) transport; (3) preliminary on-site activities (including optional ground-control-point collection); (4) pre-flight setup and checks; (5) conducting the mission; (6) data processing; and (7) mapping and change detection. The application of the proposed framework has been illustrated by a mapping case study on the glacial foreland of H\&oslash;rbyebreen, Svalbard, Norway. A consumer-grade quadcopter (DJI Phantom) was used to collect the data, while images were processed using the structure-from-motion approach. The resultant orthomosaic (1.9 cm ground sampling distance\&mdash;GSD) and digital elevation model (7.9 cm GSD) were used to map the glacial-related landforms in detail. It demonstrated the applicability of the proposed framework to map and potentially monitor detailed changes in a rapidly evolving proglacial environment, using a low-cost approach. Its coverage of multiple aspects ensures that the proposed framework is universal and can be applied in a broader range of settings.},
  issue = {1},
  langid = {english},
  keywords = {drone,GIS,glacial geomorphology,mapping,SfM,UAV},
  file = {C\:\\Users\\Peter\\Zotero\\storage\\FINBUPP8\\Ewertowski et al. - 2019 - Operational Framework for Rapid, Very-high Resolut.pdf;C\:\\Users\\Peter\\Zotero\\storage\\NCXZCXRW\\65.html}
}

