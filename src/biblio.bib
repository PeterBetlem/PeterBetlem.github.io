@article{peaceReviewPangaeaDispersal2020,
  title = {A Review of {{Pangaea}} Dispersal and {{Large Igneous Provinces}} – {{In}} Search of a Causative Mechanism},
  author = {Peace, Alexander L. and Phethean, J. J. J. and Franke, D. and Foulger, G. R. and Schiffer, C. and Welford, J. K. and McHone, G. and Rocchi, S. and Schnabel, M. and Doré, A. G.},
  date = {2020-07-01},
  journaltitle = {Earth-Science Reviews},
  shortjournal = {Earth-Science Reviews},
  series = {A New Paradigm for the {{North Atlantic Realm}}},
  volume = {206},
  pages = {102902},
  issn = {0012-8252},
  doi = {10.1016/j.earscirev.2019.102902},
  url = {https://www.sciencedirect.com/science/article/pii/S0012825219304817},
  urldate = {2021-08-04},
  abstract = {The breakup of Pangaea was accompanied by extensive, episodic, magmatic activity. Several Large Igneous Provinces (LIPs) formed, such as the Central Atlantic Magmatic Province (CAMP) and the North Atlantic Igneous Province (NAIP). Here, we review the chronology of Pangaea breakup and related large-scale magmatism. We review the Triassic formation of the Central Atlantic Ocean, the breakup between East and West Gondwana in the Middle Jurassic, the Early Cretaceous opening of the South Atlantic, the Cretaceous separation of India from Antarctica, and finally the formation of the North Atlantic in the Mesozoic-Cenozoic. We demonstrate that throughout the dispersal of Pangaea, major volcanism typically occurs distal from the locus of rift initiation and initial oceanic crust accretion. There is no location where extension propagates away from a newly formed LIP. Instead, LIPs are coincident with major lithosphere-scale shear movements, aborted rifts and splinters of continental crust rifted far out into the oceanic domain. These observations suggest that a fundamental reappraisal of the causes and consequences of breakup-related LIPs is in order.},
  langid = {english},
  file = {C\:\\Users\\Peter\\Zotero\\storage\\6I8QXTU3\\Peace et al. - 2020 - A review of Pangaea dispersal and Large Igneous Pr.pdf;C\:\\Users\\Peter\\Zotero\\storage\\4C7E3K97\\S0012825219304817.html}
}

