@article{muckenhuberSeaIceCover2016,
  title = {Sea Ice Cover in {{Isfjorden}} and {{Hornsund}}, {{Svalbard}} (2000–2014) from Remote Sensing Data},
  author = {Muckenhuber, S. and Nilsen, F. and Korosov, A. and Sandven, S.},
