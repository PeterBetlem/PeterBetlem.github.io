@article{drewsValidationFractureData2018,
  title = {Validation of Fracture Data Recognition in Rock Masses by Automated Plane Detection in {{3D}} Point Clouds},
  author = {Drews, T. and Miernik, G. and Anders, K. and Höfle, B. and Profe, J. and Emmerich, A. and Bechstädt, T.},
  date = {2018-09-01},
  journaltitle = {International Journal of Rock Mechanics and Mining Sciences},
  shortjournal = {International Journal of Rock Mechanics and Mining Sciences},
