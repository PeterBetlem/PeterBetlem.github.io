@article{clarkeDavisStraitPaleocene2020,
  title = {Davis {{Strait Paleocene}} Picrites: {{Products}} of a Plume or Plates?},
  shorttitle = {Davis {{Strait Paleocene}} Picrites},
  author = {Clarke, D. Barrie and Beutel, Erin K.},
  date = {2020-07-01},
