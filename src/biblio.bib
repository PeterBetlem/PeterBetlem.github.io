@inproceedings{betlemSeismicModellingVirtual2020,
  title = {Seismic {{Modelling}} of {{Virtual Outcrops}}: {{Application}} of {{Rock Physics Beyond}} the {{Borehole}}},
  shorttitle = {Seismic {{Modelling}} of {{Virtual Outcrops}}},
  author = {Betlem, P. and Rabbel, O. and Lecomte, I. and Senger, K.},
  date = {2020-02-10},
  volume = {2020},
  number = {1},
  pages = {1--5},
  publisher = {{European Association of Geoscientists \& Engineers}},
  issn = {2214-4609},
  doi = {10.3997/2214-4609.2020603039},
  url = {https://www.earthdoc.org/content/papers/10.3997/2214-4609.2020603039},
  urldate = {2020-10-08},
  abstract = {Summary Photogrammetry-derived virtual outcrop models provide cost-effective, high-resolution representations of outcrops and can be used to quantitatively characterise the geometry of structural and sedimentological features. Used in conjunction with rock physics, realistic geological models are obtained that are suitable as input to synthetic geophysics (e.g., seismic or electromagnetic modelling). Here we detail the workflow by implementing a case study from central Spitsbergen, Svalbard. The case study targets the seismic characteristics of various contact metamorphic aureole types associated with Early Cretaceous igneous intrusions. The bottom-up approach successfully integrates outcrops, wireline logging, and seismic data through use of well-derived rock physics relations, which is likely applicable and to the benefit of anything from carbonates to sandstone reservoirs. The extension of rock physics beyond borehole data is highly beneficial to geophysical interpretations as it allows for an improved quantification of uncertainties that is critical to proper basin and petroleum system analyses, especially in unconventional settings.},
  eventtitle = {Fifth {{EAGE Workshop}} on {{Rock Physics}}},
  isbn = {978-2-02-060303-4},
  langid = {english},
  file = {C\:\\Users\\Peter\\Zotero\\storage\\VEIAM8D7\\2214-4609.html}
}

