@article{shakhovaUnderstandingPermafrostHydrate2019,
  title = {Understanding the {{Permafrost}}–{{Hydrate System}} and {{Associated Methane Releases}} in the {{East Siberian Arctic Shelf}}},
  author = {Shakhova, Natalia and Semiletov, Igor and Chuvilin, Evgeny},
  date = {2019-06},
